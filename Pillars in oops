public class Pillar{
    String color;
    void eat(){
        System.out.println("eats");

    }
    void breathe(){
        System.out.println("breathes");
    }

//    derived class

   static class mammal extends Pillar{
        int legs;

        }
    static class dog extends mammal{
        String breed;
    }

    public static void main(String[] args) {
        dog c= new dog();
        c.color="red";
        System.out.println(c.color);
    }
}
