public class LsQues {
    public static void main(String[] args) {
        String name= " garima";
        char target= 'p';
        System.out.println(search(name, target));

    }
    static Boolean search(String str, char target){
        if (str.length()== 0){
            return null;
        }
        for (int i =0 ; i< str.length(); i++){
           if (target== str.charAt(i)){
               return true;

           }
        }
        return false;
        }
    }

