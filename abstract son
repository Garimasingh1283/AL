public class Son extends Parent1 {
    public Son(int age){
       super(23);
//       we used super instead of this.age
    }
    @Override
void career(){
        System.out.println("i am going to be a doctor");

        }
        @Override
void partner(){
            System.out.println("garima" );

        }
}
public class Main {
    public static void main(String[] args) {
       Son son= new Son(24);
       son.career();
        System.out.println();
    }
// WE CANT CALL THE CONSTRUCTOR OF ABSTRACT CLASS IN MAIN CLASS
//    Parent1 mom= new Parent1() will give error
}
