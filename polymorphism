public class Polymorphism {
    public static void main(String[] args) {
//        calculator cal= new calculator();
//        this is called method overloading, because before calling this
//        constructor compiler knew
//                that we which function is need to
//                be called
//        System.out.println(cal.sum(1,2));
//        System.out.println(cal.sum(12,2,3));
        Deer d= new Deer();
        d.eat();
    }
//     static class calculator {
//         int sum(int a, int b) {
//             return a + b;
//         }
//
//         int sum(int a, int b, int c) {
//             return a + b + c;
//         }
//     }


//method overriding
static class Animal{
        void eat(){
            System.out.println("eats food");
        }

}
static class Deer extends Animal{
        void eat(){
            System.out.println("eats grass");
        }
}

}
