public class A {
    protected int num;
    String name;
    int[] arr;

    public int getNum() {
        return num;

    }

public void setNum(int num){
    this.num= num;
    }
    public A(int num, String name)
    {
        this.num= num;
        this.name= name;
        this.arr=new int[num];
    }
}
public class Main3 {
    public static void main(String[] args) {
        A obj = new A(12,"garima");

        obj.getNum();
        int n= obj.num;
    }
}
