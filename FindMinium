public class FindMin {
    public static void main(String[] args) {
        int []arr ={56,45,6,7,87,22};
        System.out.println(FindMinimum(arr));


    }
    static int FindMinimum(int[]arr){
        if ((arr.length==0)){
            return -1;
        }
        int ans= arr[0];
        for (int i =0; i< arr.length; i++){
            if (ans> arr[i]){
                ans= arr[i];
            }
        }

return ans;
    }

}
